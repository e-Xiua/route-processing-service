# Variables de entorno para route-processing-service

# Puerto del servidor (interno: 8086, externo: 8086)
SERVER_PORT=8086
ROUTE_PROCESSING_PORT=8086

# CORS Origins
CORS_ORIGIN_1=http://localhost:4200
CORS_ORIGIN_2=http://localhost:3000
CORS_ORIGIN_3=http://localhost:8085

# gRPC Python MRL-AMIS Service Configuration
GRPC_PYTHON_HOST=mrl_amis_python_service
GRPC_PYTHON_PORT=50051
GRPC_CONNECTION_TIMEOUT=30
GRPC_REQUEST_TIMEOUT=600
GRPC_MAX_RETRIES=3

# Processing Configuration
MAX_CONCURRENT_REQUESTS=5
CLEANUP_HOURS=2

# Java Options
JAVA_OPTS_XMS=512m
JAVA_OPTS_XMX=1024m
